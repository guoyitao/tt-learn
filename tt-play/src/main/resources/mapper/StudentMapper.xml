<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="generator.mapper.StudentMapper">

    <resultMap id="BaseResultMap" type="generator.domain.Student">
            <id property="sid" column="sid" jdbcType="INTEGER"/>
            <result property="cid" column="cid" jdbcType="INTEGER"/>
            <result property="sname" column="sname" jdbcType="VARCHAR"/>
            <result property="ssex" column="ssex" jdbcType="VARCHAR"/>
            <result property="saddress" column="saddress" jdbcType="VARCHAR"/>
            <result property="stel" column="stel" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        sid,cid,sname,
        ssex,saddress,stel
    </sql>
</mapper>
